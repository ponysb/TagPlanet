{
    "manifest_version": 3,
    "name": "标签星球-给你的浏览器装个仪表盘",
    "version": "1.0",
    "description": "一个浏览器仪表盘，可以显示你常用的标签、历史记录、书签等信息。",
    "permissions": [
      "bookmarks",
      "history",
      "tabs"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  